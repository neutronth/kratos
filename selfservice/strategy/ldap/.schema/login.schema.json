{
  "$id":
    "https://schemas.ory.sh/kratos/selfservice/strategy/ldap/login.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["ldap_password", "ldap_identifier", "method"],
  "properties": {
    "csrf_token": {
      "type": "string"
    },
    "ldap_password": {
      "type": "string",
      "minLength": 1
    },
    "ldap_identifier": {
      "type": "string",
      "minLength": 1
    },
    "method": {
      "type": "string"
    }
  }
}
